          revision="refs/heads/Ursamoon" />

  <remote name="gitlab/hentaiOS"
          fetch="https://gitlab.hentaios.com/hentaiOS"
          revision="refs/heads/Ursamoon" />

  <remote name="github/LineageOS"
          fetch="https://github.com/LineageOS"
          revision="refs/heads/lineage-21.0" />

